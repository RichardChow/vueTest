{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'VersionUpdateConfigPage',\n  data() {\n    return {\n      config: {\n        upgradeType: '',\n        workType: '',\n        neEnvs: []\n      }\n    };\n  },\n  methods: {\n    loadConfig() {\n      const savedConfig = localStorage.getItem('upgradeConfig');\n      if (savedConfig) {\n        this.config = JSON.parse(savedConfig);\n        if (!this.config.neEnvs) {\n          this.config.neEnvs = [];\n        }\n      }\n    },\n    saveConfig() {\n      localStorage.setItem('upgradeConfig', JSON.stringify(this.config));\n      window.dispatchEvent(new Event('upgradeConfigUpdated'));\n      alert('配置已保存');\n      this.$router.push('/version-upgrade');\n    },\n    goBack() {\n      this.$router.push('/version-upgrade');\n    },\n    addEnv() {\n      this.config.neEnvs.push({\n        name: '',\n        ipList: ''\n      });\n    },\n    removeEnv(index) {\n      this.config.neEnvs.splice(index, 1);\n    }\n  },\n  mounted() {\n    this.loadConfig();\n  }\n};","map":{"version":3,"names":["name","data","config","upgradeType","workType","neEnvs","methods","loadConfig","savedConfig","localStorage","getItem","JSON","parse","saveConfig","setItem","stringify","window","dispatchEvent","Event","alert","$router","push","goBack","addEnv","ipList","removeEnv","index","splice","mounted"],"sources":["src/views/VersionUpdateConfigPage.vue"],"sourcesContent":["<template>\r\n  <div class=\"config-page\">\r\n    <h2>配置</h2>\r\n    <div class=\"config-form\">\r\n      <div class=\"form-group\">\r\n        <label for=\"upgradeType\">Upgrade Type:</label>\r\n        <textarea id=\"upgradeType\" v-model=\"config.upgradeType\" placeholder=\"输入 Upgrade Type，每行一个类型\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"workType\">Work Type:</label>\r\n        <textarea id=\"workType\" v-model=\"config.workType\" placeholder=\"输入 Work Type，每行一个类型\"></textarea>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label>NE Env:</label>\r\n        <div v-for=\"(env, index) in config.neEnvs\" :key=\"index\" class=\"ne-env-group\">\r\n          <input type=\"text\" v-model=\"env.name\" placeholder=\"环境名称\">\r\n          <input type=\"text\" v-model=\"env.ipList\" placeholder=\"IP列表，用逗号分隔\">\r\n          <button @click=\"removeEnv(index)\">删除</button>\r\n        </div>\r\n        <button @click=\"addEnv\">添加环境</button>\r\n      </div>\r\n    </div>\r\n    <div class=\"button-group\">\r\n      <button @click=\"saveConfig\" class=\"save-button\">保存配置</button>\r\n      <button @click=\"goBack\" class=\"back-button\">返回</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'VersionUpdateConfigPage',\r\n  data() {\r\n    return {\r\n      config: {\r\n        upgradeType: '',\r\n        workType: '',\r\n        neEnvs: []\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    loadConfig() {\r\n      const savedConfig = localStorage.getItem('upgradeConfig');\r\n      if (savedConfig) {\r\n        this.config = JSON.parse(savedConfig);\r\n        if (!this.config.neEnvs) {\r\n          this.config.neEnvs = [];\r\n        }\r\n      }\r\n    },\r\n    saveConfig() {\r\n      localStorage.setItem('upgradeConfig', JSON.stringify(this.config));\r\n      window.dispatchEvent(new Event('upgradeConfigUpdated'));\r\n      alert('配置已保存');\r\n      this.$router.push('/version-upgrade');\r\n    },\r\n    goBack() {\r\n      this.$router.push('/version-upgrade');\r\n    },\r\n    addEnv() {\r\n      this.config.neEnvs.push({ name: '', ipList: '' });\r\n    },\r\n    removeEnv(index) {\r\n      this.config.neEnvs.splice(index, 1);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadConfig();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n/* 保留原有样式 */\r\n\r\n.ne-env-group {\r\n  display: flex;\r\n  gap: 10px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.ne-env-group input {\r\n  flex: 1;\r\n}\r\n\r\n.ne-env-group button {\r\n  flex-shrink: 0;\r\n}\r\n</style>\r\n"],"mappings":";AA8BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;QACAC,WAAA;QACAC,QAAA;QACAC,MAAA;MACA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,MAAAC,WAAA,GAAAC,YAAA,CAAAC,OAAA;MACA,IAAAF,WAAA;QACA,KAAAN,MAAA,GAAAS,IAAA,CAAAC,KAAA,CAAAJ,WAAA;QACA,UAAAN,MAAA,CAAAG,MAAA;UACA,KAAAH,MAAA,CAAAG,MAAA;QACA;MACA;IACA;IACAQ,WAAA;MACAJ,YAAA,CAAAK,OAAA,kBAAAH,IAAA,CAAAI,SAAA,MAAAb,MAAA;MACAc,MAAA,CAAAC,aAAA,KAAAC,KAAA;MACAC,KAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,OAAA;MACA,KAAAF,OAAA,CAAAC,IAAA;IACA;IACAE,OAAA;MACA,KAAArB,MAAA,CAAAG,MAAA,CAAAgB,IAAA;QAAArB,IAAA;QAAAwB,MAAA;MAAA;IACA;IACAC,UAAAC,KAAA;MACA,KAAAxB,MAAA,CAAAG,MAAA,CAAAsB,MAAA,CAAAD,KAAA;IACA;EACA;EACAE,QAAA;IACA,KAAArB,UAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}