{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nconst CHINESE = '中文';\nconst ENGLISH = 'English';\nexport default {\n  name: 'Dashboard',\n  data() {\n    return {\n      currentLanguage: CHINESE,\n      isHomePage: true\n    };\n  },\n  methods: {\n    logout() {\n      console.log('开始登出操作');\n      this.$router.push('/');\n      console.log('用户已成功登出，已跳转到首页');\n    },\n    toggleLanguage() {\n      this.currentLanguage = this.currentLanguage === CHINESE ? ENGLISH : CHINESE;\n      console.log('语言切换完成');\n    },\n    goBack() {\n      this.$router.push('/dashboard');\n      console.log('返回首页');\n    }\n  },\n  computed: {\n    isHomePage() {\n      return this.$route.path === '/dashboard';\n    }\n  },\n  mounted() {\n    console.log('Dashboard 组件已加载');\n    console.log('当前语言：', this.currentLanguage);\n    console.log('应用正在运行于：', window.location.href);\n  },\n  created() {\n    console.log('Dashboard 组件已创建');\n    console.log('Vue 版本：', this.$vue.version);\n    this.isHomePage = this.$route.path === '/dashboard';\n  }\n};","map":{"version":3,"names":["CHINESE","ENGLISH","name","data","currentLanguage","isHomePage","methods","logout","console","log","$router","push","toggleLanguage","goBack","computed","$route","path","mounted","window","location","href","created","$vue","version"],"sources":["src/views/Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"dashboard\">\r\n    <div class=\"sidebar\">\r\n      <h2>Dashboard</h2>\r\n      <nav>\r\n        <router-link to=\"/dashboard\" exact>\r\n          <i class=\"fas fa-home\"></i> 首页\r\n        </router-link>\r\n        <router-link to=\"/version-upgrade\">\r\n          <i class=\"fas fa-upload\"></i> 更新版本\r\n        </router-link>\r\n        <router-link to=\"/element-query\">\r\n          <i class=\"fas fa-search\"></i> 设备查询\r\n        </router-link>\r\n        <router-link to=\"/test-report\">\r\n          <i class=\"fas fa-chart-bar\"></i> 报告统计\r\n        </router-link>\r\n      </nav>\r\n    </div>\r\n    <div class=\"main-content\">\r\n      <div v-if=\"!isHomePage\" class=\"back-button\">\r\n        <button @click=\"goBack\" class=\"btn-back\">\r\n          <i class=\"fas fa-arrow-left\"></i> 返回首页\r\n        </button>\r\n      </div>\r\n      <div class=\"image-slider\">\r\n        <!-- 这里可以添加图片滚动组件 -->\r\n        <div class=\"placeholder\">图片滚动区域</div>\r\n      </div>\r\n      <h2>欢迎来到仪表板</h2>\r\n      <!-- 这里可以添加仪表板的主要内容 -->\r\n    </div>\r\n    <div class=\"toolbar\">\r\n      <button @click=\"logout\">\r\n        <i class=\"fas fa-sign-out-alt\"></i> Log out\r\n      </button>\r\n      <button @click=\"toggleLanguage\">\r\n        <i class=\"fas fa-language\"></i> {{ currentLanguage }}\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nconst CHINESE = '中文';\r\nconst ENGLISH = 'English';\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  data() {\r\n    return {\r\n      currentLanguage: CHINESE,\r\n      isHomePage: true\r\n    }\r\n  },\r\n  methods: {\r\n    logout() {\r\n      console.log('开始登出操作');\r\n      this.$router.push('/');\r\n      console.log('用户已成功登出，已跳转到首页');\r\n    },\r\n    toggleLanguage() {\r\n      this.currentLanguage = this.currentLanguage === CHINESE ? ENGLISH : CHINESE;\r\n      console.log('语言切换完成');\r\n    },\r\n    goBack() {\r\n      this.$router.push('/dashboard');\r\n      console.log('返回首页');\r\n    }\r\n  },\r\n  computed: {\r\n    isHomePage() {\r\n      return this.$route.path === '/dashboard';\r\n    }\r\n  },\r\n  mounted() {\r\n    console.log('Dashboard 组件已加载');\r\n    console.log('当前语言：', this.currentLanguage);\r\n    console.log('应用正在运行于：', window.location.href);\r\n  },\r\n  created() {\r\n    console.log('Dashboard 组件已创建');\r\n    console.log('Vue 版本：', this.$vue.version);\r\n    this.isHomePage = this.$route.path === '/dashboard';\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard {\r\n  display: flex;\r\n  height: 100vh;\r\n}\r\n\r\n.sidebar {\r\n  width: 200px;\r\n  background-color: #f0f0f0;\r\n  padding: 20px;\r\n}\r\n\r\n.sidebar h2 {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.sidebar nav {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.sidebar nav a {\r\n  margin-bottom: 10px;\r\n  text-decoration: none;\r\n  color: #333;\r\n  padding: 10px;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.sidebar nav a:hover,\r\n.sidebar nav a.router-link-active {\r\n  background-color: #e0e0e0;\r\n}\r\n\r\n.sidebar nav a i {\r\n  margin-right: 10px;\r\n}\r\n\r\n.main-content {\r\n  flex-grow: 1;\r\n  padding: 20px;\r\n  overflow-y: auto;\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.image-slider {\r\n  height: 200px;\r\n  background-color: #e0e0e0;\r\n  margin-bottom: 20px;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.placeholder {\r\n  color: #666;\r\n}\r\n\r\n.toolbar {\r\n  width: 100px;\r\n  background-color: #f0f0f0;\r\n  padding: 20px;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.toolbar button {\r\n  margin-bottom: 10px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.toolbar button i {\r\n  margin-right: 5px;\r\n}\r\n\r\n.back-button {\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.btn-back {\r\n  background-color: #4CAF50; /* 绿色 */\r\n  color: white;\r\n  border: none;\r\n  padding: 10px 20px;\r\n  text-align: center;\r\n  text-decoration: none;\r\n  display: inline-block;\r\n  font-size: 16px;\r\n  margin: 4px 2px;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n  transition: background-color 0.3s;\r\n}\r\n\r\n.btn-back:hover {\r\n  background-color: #45a049;\r\n}\r\n\r\n.btn-back i {\r\n  margin-right: 5px;\r\n}\r\n</style>"],"mappings":";AA4CA,MAAAA,OAAA;AACA,MAAAC,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,eAAA,EAAAJ,OAAA;MACAK,UAAA;IACA;EACA;EACAC,OAAA;IACAC,OAAA;MACAC,OAAA,CAAAC,GAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;MACAH,OAAA,CAAAC,GAAA;IACA;IACAG,eAAA;MACA,KAAAR,eAAA,QAAA