{"ast":null,"code":"export default {\n  name: 'VersionUpgrade',\n  data() {\n    return {\n      selectedElement: '',\n      selectedVersion: '',\n      elements: [{\n        id: 1,\n        name: '网元A'\n      }, {\n        id: 2,\n        name: '网元B'\n      }, {\n        id: 3,\n        name: '网元C'\n      }],\n      versions: ['1.0.0', '1.1.0', '1.2.0', '2.0.0']\n    };\n  },\n  methods: {\n    upgradeVersion() {\n      console.log('升级版本:', this.selectedElement, this.selectedVersion);\n      // 这里添加实际的升级逻辑\n      alert(`网元 ${this.selectedElement} 已成功升级到版本 ${this.selectedVersion}`);\n    }\n  }\n};","map":{"version":3,"names":["name","data","selectedElement","selectedVersion","elements","id","versions","methods","upgradeVersion","console","log","alert"],"sources":["src/components/VersionUpgrade.vue"],"sourcesContent":["<template>\r\n  <div class=\"version-upgrade card\">\r\n    <h2>网元版本升级</h2>\r\n    <form @submit.prevent=\"upgradeVersion\">\r\n      <div>\r\n        <label for=\"element\">选择网元：</label>\r\n        <select id=\"element\" v-model=\"selectedElement\" required>\r\n          <option value=\"\">请选择网元</option>\r\n          <option v-for=\"element in elements\" :key=\"element.id\" :value=\"element.id\">\r\n            {{ element.name }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n      <div>\r\n        <label for=\"version\">选择版本：</label>\r\n        <select id=\"version\" v-model=\"selectedVersion\" required>\r\n          <option value=\"\">请选择版本</option>\r\n          <option v-for=\"version in versions\" :key=\"version\" :value=\"version\">\r\n            {{ version }}\r\n          </option>\r\n        </select>\r\n      </div>\r\n      <button type=\"submit\">升级版本</button>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'VersionUpgrade',\r\n  data() {\r\n    return {\r\n      selectedElement: '',\r\n      selectedVersion: '',\r\n      elements: [\r\n        { id: 1, name: '网元A' },\r\n        { id: 2, name: '网元B' },\r\n        { id: 3, name: '网元C' },\r\n      ],\r\n      versions: ['1.0.0', '1.1.0', '1.2.0', '2.0.0']\r\n    }\r\n  },\r\n  methods: {\r\n    upgradeVersion() {\r\n      console.log('升级版本:', this.selectedElement, this.selectedVersion);\r\n      // 这里添加实际的升级逻辑\r\n      alert(`网元 ${this.selectedElement} 已成功升级到版本 ${this.selectedVersion}`);\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\nform > div {\r\n  margin-bottom: 15px;\r\n}\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 5px;\r\n}\r\nselect {\r\n  width: 100%;\r\n  padding: 10px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n}\r\n</style>"],"mappings":"AA4BA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,eAAA;MACAC,eAAA;MACAC,QAAA,GACA;QAAAC,EAAA;QAAAL,IAAA;MAAA,GACA;QAAAK,EAAA;QAAAL,IAAA;MAAA,GACA;QAAAK,EAAA;QAAAL,IAAA;MAAA,EACA;MACAM,QAAA;IACA;EACA;EACAC,OAAA;IACAC,eAAA;MACAC,OAAA,CAAAC,GAAA,eAAAR,eAAA,OAAAC,eAAA;MACA;MACAQ,KAAA,YAAAT,eAAA,kBAAAC,eAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}