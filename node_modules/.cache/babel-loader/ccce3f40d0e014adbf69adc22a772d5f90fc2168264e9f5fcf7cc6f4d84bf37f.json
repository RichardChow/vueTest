{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'Config',\n  data() {\n    return {\n      config: {\n        upgradeType: 'normal',\n        workType: 'type1',\n        ipLists: {}\n      },\n      newEnv: ''\n    };\n  },\n  methods: {\n    loadConfig() {\n      const savedConfig = localStorage.getItem('upgradeConfig');\n      if (savedConfig) {\n        this.config = JSON.parse(savedConfig);\n      }\n    },\n    saveConfig() {\n      localStorage.setItem('upgradeConfig', JSON.stringify(this.config));\n      alert('配置已保存');\n    },\n    goBack() {\n      this.$router.push('/version-upgrade');\n    },\n    addEnv() {\n      if (this.newEnv && !this.config.ipLists[this.newEnv]) {\n        this.$set(this.config.ipLists, this.newEnv, '');\n        this.newEnv = '';\n      }\n    },\n    removeEnv(env) {\n      this.$delete(this.config.ipLists, env);\n    }\n  },\n  mounted() {\n    this.loadConfig();\n  }\n};","map":{"version":3,"names":["name","data","config","upgradeType","workType","ipLists","newEnv","methods","loadConfig","savedConfig","localStorage","getItem","JSON","parse","saveConfig","setItem","stringify","alert","goBack","$router","push","addEnv","$set","removeEnv","env","$delete","mounted"],"sources":["src/views/UpdateVersionConfigPage.vue"],"sourcesContent":["<template>\r\n    <div class=\"config-page\">\r\n      <h2>配置</h2>\r\n      <div class=\"config-form\">\r\n        <div class=\"form-group\">\r\n          <label for=\"upgradeType\">Upgrade Type:</label>\r\n          <select id=\"upgradeType\" v-model=\"config.upgradeType\">\r\n            <option value=\"normal\">Normal</option>\r\n            <option value=\"force\">Force</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"workType\">Work Type:</label>\r\n          <select id=\"workType\" v-model=\"config.workType\">\r\n            <option value=\"type1\">Type 1</option>\r\n            <option value=\"type2\">Type 2</option>\r\n            <option value=\"type3\">Type 3</option>\r\n          </select>\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"neEnv\">NE Env:</label>\r\n          <input type=\"text\" id=\"neEnv\" v-model=\"newEnv\" placeholder=\"输入新的环境名称\">\r\n          <button @click=\"addEnv\">添加环境</button>\r\n        </div>\r\n        <div v-for=\"(ipList, env) in config.ipLists\" :key=\"env\" class=\"form-group\">\r\n          <label :for=\"env\">{{ env }} IP List:</label>\r\n          <input :id=\"env\" v-model=\"config.ipLists[env]\" placeholder=\"IP1,IP2,IP3\">\r\n          <button @click=\"removeEnv(env)\">删除环境</button>\r\n        </div>\r\n      </div>\r\n      <div class=\"button-group\">\r\n        <button @click=\"saveConfig\" class=\"save-button\">保存配置</button>\r\n        <button @click=\"goBack\" class=\"back-button\">返回</button>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    name: 'Config',\r\n    data() {\r\n      return {\r\n        config: {\r\n          upgradeType: 'normal',\r\n          workType: 'type1',\r\n          ipLists: {}\r\n        },\r\n        newEnv: ''\r\n      }\r\n    },\r\n    methods: {\r\n      loadConfig() {\r\n        const savedConfig = localStorage.getItem('upgradeConfig');\r\n        if (savedConfig) {\r\n          this.config = JSON.parse(savedConfig);\r\n        }\r\n      },\r\n      saveConfig() {\r\n        localStorage.setItem('upgradeConfig', JSON.stringify(this.config));\r\n        alert('配置已保存');\r\n      },\r\n      goBack() {\r\n        this.$router.push('/version-upgrade');\r\n      },\r\n      addEnv() {\r\n        if (this.newEnv && !this.config.ipLists[this.newEnv]) {\r\n          this.$set(this.config.ipLists, this.newEnv, '');\r\n          this.newEnv = '';\r\n        }\r\n      },\r\n      removeEnv(env) {\r\n        this.$delete(this.config.ipLists, env);\r\n      }\r\n    },\r\n    mounted() {\r\n      this.loadConfig();\r\n    }\r\n  }\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .config-page {\r\n    padding: 20px;\r\n  }\r\n  \r\n  .config-form {\r\n    max-width: 500px;\r\n    margin: 0 auto;\r\n  }\r\n  \r\n  .form-group {\r\n    margin-bottom: 15px;\r\n  }\r\n  \r\n  label {\r\n    display: block;\r\n    margin-bottom: 5px;\r\n  }\r\n  \r\n  input, select {\r\n    width: 100%;\r\n    padding: 8px;\r\n    border: 1px solid #ddd;\r\n    border-radius: 4px;\r\n  }\r\n  \r\n  button {\r\n    padding: 8px 15px;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    margin-right: 10px;\r\n  }\r\n  \r\n  .save-button {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n  }\r\n  \r\n  .back-button {\r\n    background-color: #3498db;\r\n    color: white;\r\n  }\r\n  \r\n  .button-group {\r\n    margin-top: 20px;\r\n    text-align: center;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAsCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;QACAC,WAAA;QACAC,QAAA;QACAC,OAAA;MACA;MACAC,MAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,MAAAC,WAAA,GAAAC,YAAA,CAAAC,OAAA;MACA,IAAAF,WAAA;QACA,KAAAP,MAAA,GAAAU,IAAA,CAAAC,KAAA,CAAAJ,WAAA;MACA;IACA;IACAK,WAAA;MACAJ,YAAA,CAAAK,OAAA,kBAAAH,IAAA,CAAAI,SAAA,MAAAd,MAAA;MACAe,KAAA;IACA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;IACAC,OAAA;MACA,SAAAf,MAAA,UAAAJ,MAAA,CAAAG,OAAA,MAAAC,MAAA;QACA,KAAAgB,IAAA,MAAApB,MAAA,CAAAG,OAAA,OAAAC,MAAA;QACA,KAAAA,MAAA;MACA;IACA;IACAiB,UAAAC,GAAA;MACA,KAAAC,OAAA,MAAAvB,MAAA,CAAAG,OAAA,EAAAmB,GAAA;IACA;EACA;EACAE,QAAA;IACA,KAAAlB,UAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}